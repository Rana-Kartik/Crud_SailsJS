
<h1>Articles</h1>
<table border="2">
    <thead>
        <tr>
            <th>Title</th>
            <th>Body</th>
            <th>Delete</th>
            <th>Edit</th>
        </tr>
    </thead>
    <tbody>
        <% test.forEach(function(tt){ %>
            <tr>
                <td><%= tt.title %></td>
                <td><%= tt.body %></td>
                <td>
                 <form method="post" action="/test/delete/<%= tt.id %>">
                    <input type="submit" value="Delete" class="btn btn-danger">
                 </form>
                </td>
                <td><a href="/test/update/<%= tt.id %>">Edit</a></td>    
            </tr>
        <% }) %>
     </tbody>
</table>    